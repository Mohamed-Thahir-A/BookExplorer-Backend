{
  "name": "backend",
  "version": "0.0.1",
  "description": "Product Data Explorer Backend",
  "author": "Mohamed Thahir",
  "private": true,
  "license": "MIT",
"scripts": {
  "preinstall": "mkdir -p /usr/src/app/.playwright",
  "postinstall": "PLAYWRIGHT_BROWSERS_PATH=/usr/src/app/.playwright npx playwright install chromium",
  "build": "node node_modules/@nestjs/cli/bin/nest.js build",
  "start": "node dist/main",
  "dev": "node ./node_modules/@nestjs/cli/bin/nest.js start --watch",
  "start:dev": "node ./node_modules/@nestjs/cli/bin/nest.js start --watch",
  "start:prod": "node dist/main",
  "start:debug": "node ./node_modules/@nestjs/cli/bin/nest.js start --debug --watch",
  "typeorm": "ts-node ./node_modules/typeorm/cli",
  "migration:generate": "npm run typeorm -- migration:generate -d src/data-source.ts",
  "migration:create": "npm run typeorm -- migration:create",
  "migration:run": "npm run typeorm -- migration:run -d src/data-source.ts",
  "migration:revert": "npm run typeorm -- migration:revert -d src/data-source.ts",
  "migration:show": "npm run typeorm -- migration:show -d src/data-source.ts"
},
  "dependencies": {
    "@nestjs/cli": "^10.4.9",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.0.0",
    "@nestjs/throttler": "^5.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "@supabase/supabase-js": "^2.58.0",
    "bcrypt": "^6.0.0",
    "cache-manager": "^7.2.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "crawlee": "^3.15.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "playwright": "^1.48.0",
    "reflect-metadata": "^0.1.0",
    "rxjs": "^7.0.0",
    "typeorm": "^0.3.20",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^10.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^4.17.0",
    "@types/node": "^20.0.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/uuid": "^10.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  }
}
